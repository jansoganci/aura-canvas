{
  "name": "aura-canvas-ai-image-editor",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd worker-api && npx wrangler dev --port 8787",
    "build": "next build",
    "start": "next start",
    "deploy": "npm run build && wrangler pages deploy out",
    "deploy:api": "cd worker-api && wrangler deploy",
    "deploy:all": "npm run deploy:api && npm run deploy",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "browser-image-compression": "^2.0.2",
    "nanoid": "^5.1.6",
    "next": "^15.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.2",
    "wrangler": "^4.49.0"
  }
}
